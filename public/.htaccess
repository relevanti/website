# SPA fallback на index.html
<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  # Отдаём реальные файлы/папки как есть
  RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d
  RewriteRule ^ - [L]
  # Всё остальное — на index.html
  RewriteRule . /index.html [L]
</IfModule>

